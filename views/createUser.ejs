<!DOCTYPE html>
<html lang="en">

<head>

	<title>Dashboard</title>
	<% include partials/headadmin.ejs %>

</head>

<body class="animsition">
<div class="page-wrapper">
	<!-- HEADER MOBILE-->
	<header class="header-mobile d-block d-lg-none">
		<% include partials/headeradmin.ejs %>
	</header>
	<!-- END HEADER MOBILE-->

	<!-- MENU SIDEBAR-->
	<aside class="menu-sidebar d-none d-lg-block">
		<% include partials/menusidebar.ejs %>
	</aside>
	<!-- END MENU SIDEBAR-->

	<!-- PAGE CONTAINER-->
	<div class="page-container">
		<!-- HEADER DESKTOP-->
		<header class="header-desktop">
			<% include partials/headdesktop.ejs %>
		</header>
		<!-- HEADER DESKTOP-->

        <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="au-card recent-report">
                                    <div class="au-card-inner">
                                        <h3 class="title-2 m-b-25">Đăng kí người dùng mới</h3>
                                        <form action="/users/create" method="POST">
                                            <div class="form-group">
                                                <input id="txtFullName" name="txtFullName" type="text" class="form-control cc-exp" placeholder="Họ và tên" autocomplete="cc-exp">
                                            </div>
                                            <div class="form-group">
                                                <input id="txtEmail" name="txtEmail" type="email" class="form-control cc-exp" placeholder="Địa chỉ email" autocomplete="cc-exp">
                                            </div>
                                            <div class="form-group">
                                                <input id="txtPhone" name="txtPhone" type="tel" class="form-control cc-exp" placeholder="Số điện thoại" autocomplete="cc-exp">
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label mb-1">Ngày sinh</label>
                                                <input id="txtDate" name="txtDate" type="date" class="form-control cc-exp" autocomplete="cc-exp">
                                            </div>
                                            <div class="form-group">
                                                <input id="txtAddress" name="txtAddress" type="text" class="form-control cc-exp" placeholder="Địa chỉ" autocomplete="cc-exp">
                                            </div>
                                            <div class="form-group">
                                                <input id="txtUsername" name="txtUsername" type="text" class="form-control cc-exp" placeholder="Username" autocomplete="cc-exp">
                                            </div>
                                            <div class="form-group">
                                                <input id="txtPass" name="txtPass" type="password" class="form-control cc-exp" placeholder="Password" autocomplete="cc-exp">
                                            </div>
                                            <div class="form-group">
                                                <input id="txtrePass" name="txtrePass" type="password" class="form-control cc-exp" placeholder="Nhập lại password" autocomplete="cc-exp">
                                            </div>
                                            <div>
                                                <button id="payment-button" class="btn btn-lg btn-info btn-block">
                                                    <i class="fas fa-paper-plane"></i>
                                                    <span id="payment-button-amount">Thêm</span>
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        

		<!-- END PAGE CONTAINER-->
	</div>

</div>

<!-- Jquery JS-->
<% include partials/jqueryScript.ejs %>

</body>

</html>
<!-- end document-->
